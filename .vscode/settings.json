{
  "debug.openExplorerOnEnd": true,
  "editor.tabSize": 2,
  "editor.rulers": [90],
  "editor.autoIndent": "full",
  "editor.cursorBlinking": "solid",
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never",
    "source.fixAll.tslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.openEditors.visible": 0,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "onFocusChange",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "npm.enableScriptExplorer": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.iconTheme": "material-icon-theme",
  "auto-close-tag.SublimeText3Mode": true,
  "html.autoClosingTags": false,
  "gitlens.menus": {
    "editorGroup": false
  },
  "ng-evergreen.upgradeChannel": "Latest",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a900c9",
    "activityBar.activeBorder": "#403600",
    "activityBar.background": "#a900c9",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#403600",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#a900c9",
    "statusBar.background": "#7e0096",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#a900c9",
    "statusBarItem.remoteBackground": "#7e0096",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#7e0096",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7e009699",
    "titleBar.inactiveForeground": "#e7e7e799",
    "commandCenter.border": "#e7e7e799"
  },
  "peacock.color": "#7e0096"
}
